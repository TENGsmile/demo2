page{
  color: #666;
  padding: 10rpx;
}

.nav{
  white-space: nowrap;
  padding: 5rpx 0;
}
.nav_item{
  padding: 20rpx 45rpx;
  font-size: 30rpx;
  display: inline-block;
}
.nav_item.active{
 border-bottom: 5rpx solid #de688b;

}

.slides swiper{
  height:220rpx;
}
.slides navigator{
  width: 100%;
  height: 100%;
}
.slides image{
  width:100%;
  height: 100%
}



.table {
  border: 0px solid darkgray;
}
.tr {
  display: flex;
  width: 1500rpx;
    font-size: 25rpx;
  justify-content: center;
  height: 3rem;
  align-items: center;
  margin-right: 20rpx
}
.td {
    width:1500rpx;
    justify-content: center;
    text-align: center;
      font-size: 25rpx;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
.th {
  width: 1500rpx;
  font-size: 25rpx;
  justify-content: center;
  background: #3366FF;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}
.scrollClass {
  display: flex;
  width: 100%;
  white-space: nowrap;
  margin-top: 23px;
  height: 100%;
  background-color: white;
}