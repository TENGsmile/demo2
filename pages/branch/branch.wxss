.table {
  border: 0px solid darkgray;
}
.tr {
  display: flex;
  width: 2500rpx;
    font-size: 25rpx;
  justify-content: center;
  height: 3rem;
  align-items: center;
  margin-right: 20rpx
}
.td {
    width:2500rpx;
    overflow: hidden;
    justify-content: center;
    text-align: center;
      font-size: 25rpx;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
.th {
  width: 2500rpx;
  font-size: 25rpx;
  justify-content: center;
  background: #3366FF;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}
.scrollClass {
  display: flex;
  width: 100%;
  white-space: nowrap;
  margin-top: 23px;
  height: 100%;
  background-color: white;
}